  <div class="profilecontent my-4">
    <div class="container bg-light d-flex justify-content-center align-items-center">

      <form class="well form-horizontal "  id="contact_form">
        <!-- <fieldset> -->

        <!-- Form Name -->
        <legend>
          <center>
            <h2 ><b>Your Profile</b></h2>
          </center>
        </legend><br>

        <!-- Text input-->

        <div class="form-group mt-4">
          <!-- <label class="col-md-4 control-label">First Name</label>   -->
          <div class=" inputGroupContainer">
            <div class="input-group mb-3">
              <span class="mx-3 my-auto">First name : </span>
              <span class="input-group-text" id="basic-addon1"><i class="fa fa-user-circle mx-1"></i> </span>
              <!-- <span class="input-group-text" id="basic-addon1"> First name : </span> -->
              <input type="text" class="form-control" name="fname" id="fname" value="<%=fname%>" readonly >
            </div>
          </div>
        </div>

        <!-- Text input-->

        <div class="form-group">
          <!-- <label class="col-md-4 control-label" >Last Name</label>  -->
          <div class=" inputGroupContainer">
            <div class="input-group mb-3">
              <span class="mx-3 my-auto">Last name : </span>
              <span class="input-group-text" id="basic-addon1"><i class="fa fa-user-circle mx-1"></i> </span>
              <!-- <span class="input-group-text" id="basic-addon1"> First name : </span> -->
              <input type="text" class="form-control" name="lname" id="lname" value="<%=lname%>" readonly>
            </div>
          </div>
        </div>

        <!-- Text input-->
        <div class="form-group">
          <!-- <label class=" control-label">E-Mail</label>   -->
          <div class=" inputGroupContainer">
            <div class="input-group mb-3">
              <span class="mx-3 my-auto">Email : </span>
              <span class="input-group-text" id="basic-addon1"><i class="fa fa-envelope mx-1"></i> </span>
              <!-- <span class="input-group-text" id="basic-addon1"> First name : </span> -->
              <input type="email" class="form-control" name="email" id="email" value="<%=email%> " readonly>
            </div>
          </div>
        </div>


        <!-- Text input-->

        <div class="form-group">
          <!-- <label class=" control-label">Contact No.</label>   -->
          <div class=" inputGroupContainer">
            <div class="input-group mb-3">
              <span class="mx-3 my-auto">Mobile No. : </span>
              <span class="input-group-text" id="basic-addon1"><i class="fa fa-phone mx-1"></i> </span>
              <!-- <span class="input-group-text" id="basic-addon1"> First name : </span> -->
              <input type="text" class="form-control" name="phone" id="phone" value="(+91)  <%=phone%>" readonly>
            </div>
          </div>
        </div>

        <div class="form-group">
          <!-- <label class=" control-label">Contact No.</label>   -->
          <div class=" inputGroupContainer">
            <div class="input-group mb-3">
              <span class="mx-3 my-auto">Age : </span>
              <span class="input-group-text" id="basic-addon1"><i class="fa fa-user-circle mx-1"></i> </span>
              <!-- <span class="input-group-text" id="basic-addon1"> First name : </span> -->
              <input type="text" class="form-control" name="age" id="age" value="<%=age%> "  readonly>
            </div>
          </div>
        </div>

        <div class="form-group md-4">
          <!-- <label class=" control-label">Department / Office</label> -->
          <div class=" selectContainer">
            <div class="input-group">
              <span class="mx-3 my-auto">Education : </span>
              <span class="input-group-text" id="basic-addon1"><i class="fa fa-user-graduate mx-1 " style="font-size: 18px;"></i> </span>
              <select name="education " class="form-select bg-light form-control selectpicker" disabled >
                <option value="0">
                  <%=education%>
                </option>
                <option value="1">High School Level</option>
                <option value="2">Under Graduate</option>
                <option value="3">Post Graduate</option>
              </select>
            </div>
          </div>
        </div>

        <div class="form-group mt-3">
          <!-- <label class=" control-label">Contact No.</label>   -->
          <div class=" col-mt-4 inputGroupContainer">
            <div class="input-group mb-3">
              <span class="mx-3 my-auto">CPI : </span>
              <span class="input-group-text" id="basic-addon1"><i class="fa fa-user-circle mx-1"></i> </span>
              <!-- <span class="input-group-text" id="basic-addon1"> First name : </span> -->
              <input type="text" class="form-control" name="cpi" id="cpi" value="<%=cpi%> " readonly>
            </div>
          </div>
        </div>

        <div class="form-group mt-3">
          <div class=" col-mt-4 inputGroupContainer">
            <div class="input-group mb-3">
              <span class="mx-3 my-auto">Github link : </span>
              <a class="d-inline mx-3" href="<%=github%>" target="_blank">
                <%=github%>
              </a>
            </div>

          </div>
        </div>
<!-- 
        <input class="  btn btn-primary " href="./editStudent.html" type="button" value="Edit Profile"> -->
        <a class=" mt-4 btn btn-primary w-100" href="./editStudent.html" role="button">Edit Profile</a>


      </form>
    </div>
  </div>

  <!-- <a class="btn btn-primary" href="./findCompanies.html" role="button">Find Companies</a> -->
